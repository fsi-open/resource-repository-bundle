{
    "name": "fsi/resource-repository-bundle",
    "type": "symfony-bundle",
    "description": "Resource repository bundle",
    "keywords": ["fsi", "resource", "repository", "bundle", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "autoload": {
        "psr-4": { "FSi\\Bundle\\ResourceRepositoryBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec",
            "tests\\FSi\\Bundle\\ResourceRepositoryBundle\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/fsi-open/phpspec.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^2.8|^3.0",
        "doctrine/doctrine-bundle": "^1.12|^2.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/doctrine-bridge": "^5.4|^6.0|^7.0",
        "symfony/form" : "^5.4|^6.0|^7.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/property-access": "^5.4|^6.0|^7.0",
        "symfony/validator": "^5.4|^6.0|^7.0",
        "twig/twig": "^3.7"
    },
    "require-dev": {
        "doctrine/orm": "^2.5|^3.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "fsi/files": "^2.0.4|^3.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "phpspec/phpspec": "^7.4|^8.0@dev",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "^6.3|^7.0"
    },
    "suggest": {
        "fsi/files": "For using web_file resource type",
        "friendsofsymfony/ckeditor-bundle": "For using ckeditor resource type"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "allow-plugins": {
            "ocramius/package-versions": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "3.0": "3.0-dev",
            "2.2": "2.2-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
