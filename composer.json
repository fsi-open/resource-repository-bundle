{
    "name": "fsi/resource-repository-bundle",
    "type": "symfony-bundle",
    "description": "Resource repository bundle",
    "keywords": ["fsi", "resource", "repository", "bundle", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "autoload": {
        "psr-4": { "FSi\\Bundle\\ResourceRepositoryBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec",
            "FSi\\Bundle\\ResourceRepositoryBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/dbal": "^2.8|^3.0",
        "doctrine/doctrine-bundle": "^1.12|^2.3",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/doctrine-bridge": "^4.4|^5.0",
        "symfony/form" : "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "twig/twig": "^2.13|^3.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "fsi/doctrine-extensions-bundle": "^2.1@dev",
        "fsi/files": "^1.1",
        "fsi/form-extensions-bundle": "^2.0@dev",
        "phpunit/phpunit": "^8.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/phpstan": "^1.9.0",
        "phpstan/phpstan-phpunit": "^1.3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "^5.2"
    },
    "suggest": {
        "fsi/doctrine-extensions-bundle": "For using fsi_file resource type",
        "fsi/files": "For using web_file resource type",
        "friendsofsymfony/ckeditor-bundle": "For using ckeditor resource type"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
