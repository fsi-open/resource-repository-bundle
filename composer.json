{
    "name": "fsi/resource-repository-bundle",
    "type": "symfony-bundle",
    "description": "Resource repository bundle",
    "keywords": ["fsi", "resource", "repository", "bundle", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.5",
        "twig/twig": "^1.34|^2.0",
        "doctrine/doctrine-bundle": "^1.4",
        "symfony/framework-bundle": "^3.0",
        "symfony/form" : "^3.0",
        "symfony/validator": "^3.0",
        "symfony/event-dispatcher": "^3.0",
        "symfony/doctrine-bridge": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/property-access": "^3.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.3",
        "egeloen/ckeditor-bundle": "^5.0",
        "phpspec/phpspec": "^2.0|^3.0",
        "fsi/doctrine-extensions": "^1.0|^2.0@dev",
        "fsi/doctrine-extensions-bundle": "^1.0|^2.0@dev",
        "fsi/form-extensions-bundle": "^2.0@dev"
    },
    "suggest": {
        "fsi/doctrine-extensions-bundle": "For using fsi_file resource type",
        "egeloen/ckeditor-bundle": "For using ckeditor resource type"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\ResourceRepositoryBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "1.1": "1.1-dev",
            "dev-master": "2.0-dev"
        }
    }
}
